# Terminal emulator
export TERMINAL="alacritty"

# Web browser
export BROWSER="firefox"

# Font for GUI or terminal
export FONT="Iosevka Nerd Font 10"

# Default editor
export EDITOR="nvim"

# Screen locker
export SCREENLOCKER="betterlockscreen -l dim"

# Screenshot directory 
export SCREENSHOT_DIR="$HOME/Pictures/Screenshots"

# LAUNCHER
export LAUNCHER="$HOME/.config/rofi/launcher.sh"

# Geolocation
export LAT=6.13 
export LONG=1.2

# custom paths
export PATH="$HOME/.local/bin:$PATH"  
